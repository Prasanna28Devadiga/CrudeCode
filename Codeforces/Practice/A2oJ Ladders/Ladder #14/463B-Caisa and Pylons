#include <bits/stdc++.h>
using namespace std;

int main() {
    
    int n=0;
    cin>>n;
    int height[n+1]={0};
    int money=0;
    int energy=0;
    height[0]=0;
    for(int i=1;i<n+1;i++){
        cin>>height[i];
        
      money=max(money,height[i]);  
    }
    
    cout<<money;
   	return 0;
}
