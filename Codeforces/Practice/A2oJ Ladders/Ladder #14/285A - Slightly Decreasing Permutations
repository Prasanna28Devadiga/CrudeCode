#include <bits/stdc++.h>
using namespace std;

int main() {
    int n=0,k=0;
    cin>>n>>k;
    int array[n]={0};
    for(int i=0;i<n;i++){
        array[i]=i+1;
        
    }
    sort(array,array+k+1,greater<int>());
    for(int i=0;i<n;i++){
        cout<<array[i]<<' ';
    }
  
  
  
  
  
  
  
  
	return 0;
}
